# Analyse des sources
sonar.sources=.

# Configuration des exclusions SONAR
sonar.exclusions=%%exclusions%%

# Encoding of the source code
sonar.sourceEncoding=UTF-8

sonar.tests=%%testsDirs%%
sonar.projectKey=%%projectKey%%
sonar.projectName=%%projectName%%
sonar.projectVersion=%%version%%
sonar.php.tests.reportPath=%%phpunitReportPath%%
sonar.php.coverage.reportPaths=%%coverageReportPath%%