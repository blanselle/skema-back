<form method="post" action="{{ path('program_delete', {'id': program.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce programme ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ program.id) }}">
    <button class="btn btn-danger action-delete"><i class="bi bi-trash"></i></button>
</form>
