<form method="post" action="{{ path('exam_inscription_delete', {'examStudent': student.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette inscription ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ student.id) }}">
    <button class="btn btn-danger action-delete"><i class="bi bi-trash"></i></button>
</form>
