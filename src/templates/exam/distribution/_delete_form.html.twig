<form method="post" action="{{ path('exam_distribution_delete', {'campus': campus.id, 'id': student.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette inscription ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ student.id) }}">
    <button class="btn btn-danger action-delete"><i class="bi bi-trash"></i></button>
</form>
